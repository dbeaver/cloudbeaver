{
  "name": "@cloudbeaver/plugin-d3js",
  "type": "module",
  "sideEffects": [
    "src/**/*.css",
    "src/**/*.scss",
    "public/**/*"
  ],
  "version": "0.1.0",
  "description": "The plugin reexports d3js library and contains utility functions and components for d3js",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf --glob dist",
    "lint": "eslint ./src/ --ext .ts,.tsx",
    "test": "core-cli-test",
    "validate-dependencies": "core-cli-validate-dependencies",
    "update-ts-references": "yarn run clean && typescript-resolve-references"
  },
  "dependencies": {
    "@cloudbeaver/core-di": "^0",
    "d3": "^7",
    "d3-drag": "^3"
  },
  "devDependencies": {
    "@types/d3": "^7",
    "@types/d3-drag": "^3",
    "typescript": "^5"
  },
  "peerDependencies": {}
}
