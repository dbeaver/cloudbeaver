{
  "name": "@cloudbeaver/product-default",
  "sideEffects": [
    "src/**/*.css",
    "src/**/*.scss",
    "public/**/*"
  ],
  "version": "24.2.2",
  "description": "CloudBeaver Community",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "browserslist": [
    "extends @cloudbeaver/browserslist-config"
  ],
  "scripts": {
    "build": "rimraf --glob lib && lerna run gql:gen --stream --parallel && tsc -b",
    "bundle": "yarn build && core-cli-build --mode=production --config ../core-cli/configs/webpack.product.config.js",
    "dev": "yarn build && concurrently -P -k \"yarn tsc -b --watch --preserveWatchOutput\" \"lerna run gql:gen:dev --stream --parallel\" \"core-cli-build serve --mode=development --progress --config=../core-cli/configs/webpack.product.dev.config.js {@}\"",
    "analyse": "yarn build && rimraf --glob lib && mkdir lib && core-cli-build --mode=production --config ../core-cli/configs/webpack.product.config.js --profile --json > ./lib/stats.json",
    "postanalyse": "core-cli-analyzer ./lib/stats.json",
    "lint": "eslint ./src/ --ext .ts,.tsx",
    "version:new": "yarn version --no-git-tag-version",
    "validate-dependencies": "core-cli-validate-dependencies",
    "update-ts-references": "yarn run clean && typescript-resolve-references"
  },
  "dependencies": {
    "@cloudbeaver/core-blocks": "^0",
    "@cloudbeaver/core-bootstrap": "^0",
    "@cloudbeaver/core-di": "^0",
    "@cloudbeaver/core-localization": "^0",
    "@cloudbeaver/core-version-update": "^0",
    "@cloudbeaver/plugin-administration": "^0",
    "@cloudbeaver/plugin-authentication": "^0",
    "@cloudbeaver/plugin-authentication-administration": "^0",
    "@cloudbeaver/plugin-browser": "^0",
    "@cloudbeaver/plugin-connection-custom": "^0",
    "@cloudbeaver/plugin-connection-search": "^0",
    "@cloudbeaver/plugin-connection-template": "^0",
    "@cloudbeaver/plugin-connections": "^0",
    "@cloudbeaver/plugin-connections-administration": "^0",
    "@cloudbeaver/plugin-data-export": "^0",
    "@cloudbeaver/plugin-data-grid": "^0",
    "@cloudbeaver/plugin-data-import": "^0",
    "@cloudbeaver/plugin-data-spreadsheet-new": "^0",
    "@cloudbeaver/plugin-data-viewer": "^0",
    "@cloudbeaver/plugin-data-viewer-result-set-grouping": "^0",
    "@cloudbeaver/plugin-data-viewer-result-trace-details": "^0",
    "@cloudbeaver/plugin-datasource-context-switch": "^0",
    "@cloudbeaver/plugin-datasource-transaction-manager": "^0",
    "@cloudbeaver/plugin-ddl-viewer": "^0",
    "@cloudbeaver/plugin-devtools": "^0",
    "@cloudbeaver/plugin-gis-viewer": "^0",
    "@cloudbeaver/plugin-help": "^0",
    "@cloudbeaver/plugin-localization": "^0",
    "@cloudbeaver/plugin-log-viewer": "^0",
    "@cloudbeaver/plugin-navigation-tabs": "^0",
    "@cloudbeaver/plugin-navigation-tree": "^0",
    "@cloudbeaver/plugin-navigation-tree-filters": "^0",
    "@cloudbeaver/plugin-navigation-tree-rm": "^0",
    "@cloudbeaver/plugin-object-viewer": "^0",
    "@cloudbeaver/plugin-product": "^0",
    "@cloudbeaver/plugin-product-information-administration": "^0",
    "@cloudbeaver/plugin-projects": "^0",
    "@cloudbeaver/plugin-resource-manager": "^0",
    "@cloudbeaver/plugin-resource-manager-administration": "^0",
    "@cloudbeaver/plugin-resource-manager-scripts": "^0",
    "@cloudbeaver/plugin-root": "^0",
    "@cloudbeaver/plugin-session-expiration": "^0",
    "@cloudbeaver/plugin-settings-administration": "^0",
    "@cloudbeaver/plugin-settings-menu": "^0",
    "@cloudbeaver/plugin-settings-menu-administration": "^0",
    "@cloudbeaver/plugin-settings-panel": "^0",
    "@cloudbeaver/plugin-sql-editor": "^0",
    "@cloudbeaver/plugin-sql-editor-navigation-tab": "^0",
    "@cloudbeaver/plugin-sql-editor-navigation-tab-script": "^0",
    "@cloudbeaver/plugin-sql-editor-new": "^0",
    "@cloudbeaver/plugin-sql-editor-screen": "^0",
    "@cloudbeaver/plugin-sql-generator": "^0",
    "@cloudbeaver/plugin-sso": "^0",
    "@cloudbeaver/plugin-task-manager": "^0",
    "@cloudbeaver/plugin-theme": "^0",
    "@cloudbeaver/plugin-tools-panel": "^0",
    "@cloudbeaver/plugin-top-app-bar": "^0",
    "@cloudbeaver/plugin-top-app-bar-administration": "^0",
    "@cloudbeaver/plugin-user-profile": "^0",
    "@cloudbeaver/plugin-user-profile-administration": "^0",
    "@cloudbeaver/plugin-user-profile-settings": "^0",
    "@cloudbeaver/plugin-version": "^0",
    "@cloudbeaver/plugin-version-update-administration": "^0",
    "react": "^18",
    "reflect-metadata": "^0"
  },
  "peerDependencies": {},
  "product": {
    "name": "CloudBeaver Community"
  },
  "devDependencies": {
    "@types/react": "^18",
    "typescript": "^5"
  }
}
