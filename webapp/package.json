{
  "name": "cloudbeaver-ce",
  "version": "1.0.0",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/core-cli/**"
    ]
  },
  "resolutions": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "mobx": "^6.12.3",
    "mobx-react-lite": "^4.0.7"
  },
  "scripts": {
    "test": "core-cli-test",
    "clean": "rimraf --glob packages/*/dist",
    "lint": "eslint --ext .ts,.tsx",
    "validate-dependencies": "core-cli-validate-dependencies './packages/*/'",
    "update-ts-references": "yarn run clean && typescript-resolve-references './packages/*/'",
    "prebuild": "lerna run gql:gen --stream --parallel",
    "prepare": "cd .. && husky ./webapp/.husky",
    "postinstall": "husky"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2",
    "husky": "^9.0.11",
    "lerna": "5.5.4",
    "mobx": "^6.12.3",
    "mobx-react-lite": "^4.0.7",
    "msw": "^2.3.0",
    "path-browserify": "~1.0.1",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reakit": "~1.3.11",
    "reflect-metadata": "^0.2.2",
    "rimraf": "~5.0.7",
    "typescript": "^5.4.5",
    "typescript-plugin-css-modules": "^5.1.0"
  },
  "dependencies": {},
  "prettier": "@cloudbeaver/prettier-config",
  "packageManager": "yarn@1.22.21"
}
