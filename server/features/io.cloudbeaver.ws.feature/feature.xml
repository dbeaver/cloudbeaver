<?xml version="1.0" encoding="UTF-8"?>
<feature
        id="io.cloudbeaver.ws.feature"
        label="%featureName"
        version="1.0.20.qualifier"
        provider-name="%providerName"
        plugin="io.cloudbeaver.model">

    <description>
        %description
    </description>

    <copyright>
        %copyright
    </copyright>

    <requires>
        <import plugin="org.eclipse.core.resources" version="0.0.0" match="greaterOrEqual"/>
        <import plugin="org.eclipse.core.runtime" version="3.7" match="greaterOrEqual"/>
        <!-- Equinox -->
        <import plugin="org.eclipse.equinox.launcher" version="0.0.0" match="greaterOrEqual"/>
        <import plugin="org.eclipse.equinox.common" version="0.0.0" match="greaterOrEqual"/>
        <import plugin="org.eclipse.equinox.event" version="0.0.0" match="greaterOrEqual"/>
        <import plugin="org.eclipse.equinox.preferences" version="0.0.0" match="greaterOrEqual"/>
        <import plugin="org.eclipse.equinox.registry" version="0.0.0" match="greaterOrEqual"/>
        <import plugin="org.eclipse.osgi.compatibility.state" version="0.0.0" match="greaterOrEqual"/>
        <!-- Main Equinox service provider -->
        <import plugin="org.apache.felix.scr" version="0.0.0" match="greaterOrEqual"/>
    </requires>

    <includes id="org.jkiss.dbeaver.runtime.feature" version="0.0.0"/>

    <plugin id="io.cloudbeaver.model" download-size="0" install-size="0" version="0.0.0" unpack="false"/>

    <!-- 3rd party deps -->
    <plugin id="com.ibm.icu.base" download-size="0" install-size="0" version="0.0.0" unpack="false"/>
    <plugin id="com.google.gson" download-size="0" install-size="0" version="0.0.0" unpack="false"/>
    <plugin id="org.jkiss.bundle.graphql.java" download-size="0" install-size="0" version="0.0.0" unpack="false"/>

    <!-- Logging -->

    <plugin id="ch.qos.logback.core" download-size="0" install-size="0" version="0.0.0" unpack="false"/>
    <plugin id="ch.qos.logback.classic" download-size="0" install-size="0" version="0.0.0" unpack="false"/>
    <plugin id="ch.qos.logback.slf4j" download-size="0" install-size="0" version="0.0.0" unpack="false"/>


</feature>
